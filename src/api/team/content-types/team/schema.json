{
  "kind": "collectionType",
  "collectionName": "teams",
  "info": {
    "singularName": "team",
    "pluralName": "teams",
    "displayName": "Team",
    "description": "Dodgeball team information"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "teamName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "teamSlug": {
      "type": "uid",
      "targetField": "teamName",
      "required": true
    },
    "countryCode": {
      "type": "string",
      "required": true
    },
    "albums": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::album.album",
      "mappedBy": "team"
    }
  }
}
