{
  "kind": "collectionType",
  "collectionName": "teams",
  "info": {
    "singularName": "team",
    "pluralName": "teams",
    "displayName": "Team",
    "description": "Dodgeball team information and photo galleries"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "teamName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "teamSlug": {
      "type": "uid",
      "targetField": "teamName",
      "required": true
    },
    "countryCode": {
      "type": "string",
      "required": true
    },
    "galleryPhotos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    }
  }
}
